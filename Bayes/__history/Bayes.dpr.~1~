program Bayes;

uses
  Vcl.Forms,
  uDiagnosa in 'uDiagnosa.pas' {foDiagnosa},
  uDM in 'uDM.pas' {dm: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfoDiagnosa, foDiagnosa);
  Application.CreateForm(Tdm, dm);
  Application.Run;
end.
